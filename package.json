  "private": true,
  "scripts": {
    "android:staging": "react-native run-android --variant 'stagingDebug'",
    "android:production": "react-native run-android --variant 'productionDebug'",
    "ios:production": "react-native run-ios --scheme 'CollX' --simulator='iPhone 14'",
    "ios:staging": "react-native run-ios --scheme 'CollX-Staging' --simulator='iPhone 14'",
    "start": "yarn run relay && react-native start --reset-cache",
    "test": "jest",
    "lint": "eslint .",
    "version": "react-native-version --never-amend",
    "android:clean": "cd android && ./gradlew clean",
    "android:staging:assemble": "cd android && ./gradlew assembleStagingRelease",
    "android:staging:bundle": "cd android && ./gradlew bundleStagingRelease",
    "android:production:assemble": "cd android && ./gradlew assembleProductionRelease",
    "android:production:bundle": "cd android && ./gradlew bundleProductionRelease",
    "android:staging:build": "cd android && ./gradlew assembleStagingRelease && ./gradlew bundleStagingRelease",
    "android:build": "cd android && ./gradlew assembleProductionRelease && ./gradlew bundleProductionRelease",
    "update-schema-prod": "get-graphql-schema https://graphql.collx.app/ > ./src/schema.graphql",
    "update-schema-stage": "get-graphql-schema https://graphql.stage.collx.app/ > ./src/schema.graphql",
    "update-schema-dev": "get-graphql-schema http://localhost:4000/ > ./src/schema.graphql",
    "relay": "yarn run relay-compiler $@",
    "eslint": "eslint src",
    "eslint-fix": "eslint --fix src",
    "nibble": "eslint-nibble src",
    "postinstall": "patch-package"
